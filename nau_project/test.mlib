<?xml version="1.0" ?>
<materialLib name="test">
	<shaders>
		<shader name="test" vs = "../nau_project/shaders/test/test.vert" 
							ps = "../nau_project/shaders/test/test.frag" />
	</shaders>
	
	<materials>

		<material name="test">
			<shader name="test">
				<values>
					<valueof uniform="m_pvm" 
							 type="RENDERER" context="CURRENT" 
							 component="PROJECTION_VIEW_MODEL" />
							 
					<valueof uniform="m_normal" 
							 type="RENDERER" context="CURRENT" 
							 component="NORMAL" />
							 
					<valueof uniform="m_view" 
							 type="RENDERER" context="CURRENT" 
							 component="VIEW" />
							 
					<valueof uniform="diffuse" 
							 type="COLOR" context="CURRENT" 
							 component="DIFFUSE" />
							 
					<valueof uniform="l_dir" 
							 type="LIGHT" context="Sun"
							 component="DIRECTION" />
				</values>
			</shader>
		</material>

	</materials>
</materialLib>